<div>users</div>
<a href="/backside/users/new">new user</a>

<table id="users-table" class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Username</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Created At</th>
			<th>Last Login</th>
		</tr>
	</thead>

	<tfoot>
		<tr>
			<th>Username</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Created At</th>
			<th>Last Login</th>
		</tr>
	</tfoot>

	<tbody>
		<tr ng-repeat="user in data" style="height:59px">
			<td>{{user.username}}</td>
			<td>{{user.name.first}}</td>
			<td>{{user.name.last}}</td>
			<td>{{user.createdAt}}</td>
			<td>{{user.lastLogin}}</td>
		</tr>
	</tbody>
</table>